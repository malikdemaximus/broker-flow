{
  "name": "airbapay",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:staging": "vue-cli-service build --mode staging",
    "build:prod": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,yml,yaml,json,graphql}\""
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.1.2",
    "ant-design-vue": "^2.1.6",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "currency.js": "^2.0.4",
    "js-cookie": "^2.2.1",
    "maska": "^1.5.0",
    "moment": "^2.29.1",
    "utf8": "^3.0.0",
    "vue": "^3.0.11",
    "vue-i18n": "^9.1.6",
    "vue-json-excel": "^0.3.0",
    "vue-json-pretty": "^2.1.1",
    "vue-json-viewer": "^2.2.19",
    "vue-request": "^1.2.4",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "lint-staged": "^10.5.4",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "8.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
